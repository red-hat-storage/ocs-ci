apiVersion: {{ api_version | default('v1beta4') }}
baseDomain: {{ base_domain | default('qe.rh-ocs.com') }}
compute:
- name: worker
  platform: {}
  replicas: {{ worker_replicas | default(3) }}
controlPlane:
  name: master
  platform: {}
  replicas: {{ master_replicas | default(3) }}
metadata:
  creationTimestamp: null
  name: '{{ cluster_name }}'
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23
  machineCIDR: 10.0.0.0/16
  networkType: OpenShiftSDN
  serviceNetwork:
  - 172.30.0.0/16
platform:
  aws:
    region: {{ region | default('us-east-2') }}
pullSecret: ''
