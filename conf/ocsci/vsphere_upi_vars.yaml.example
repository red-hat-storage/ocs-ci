  platform: 'vsphere'
  deployment_type: 'upi'
  # aws region
  region: 'us-east-2'
  base_domain: 'qe.rh-ocs.com'
  # vsphere details
  vsphere_server: 'vcenter-ocsqe.scl.lab.tlv.redhat.com'
  vsphere_user: '<user>'
