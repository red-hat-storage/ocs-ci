  ipam_token: '<token>'
  # use vm_network if you are using separate VLAN
  vm_network: 'VLAN_172'
  vm_template: 'rhcos-410.8.20190520.1'
  gateway: '<gateway>'
  dns: '<dns>'
  httpd_server: 'server_name'
  path_to_upload: '/var/www/html/ocs4qe'
